{
    "name": "Hangouts-Inset",
    "description": "Chrome extension for Google Hangouts which converts image links into actual images",
    "version": "1.1.0",
    "author": "Tibor Botos",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["lib/jquery.min.js", "content.js"],
            "all_frames": true
        }
    ],

    "icons": {
        "128": "assets/icon.png"
    },

    "manifest_version": 2
}
